<?xml version="1.0" encoding="utf-8"?>


<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="285dp"
    android:layout_height="match_parent"
    android:layout_gravity="start"

    android:orientation="vertical"
    tools:visibility="visible"> <!--android:background="@drawable/main_grad_pack"-->

    <!--app:srcCompat="@drawable/main_grad_pack" -->

    <ImageView
        android:id="@+id/bgMosque"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/ic_burget_menu"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />

    <View
        android:id="@+id/view23"
        android:layout_width="0dp"
        android:layout_marginStart="35dp"
        android:layout_height="500dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_copy1"
        app:layout_constraintVertical_bias="0.22000003" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="gone"
        app:srcCompat="@drawable/lock_icon" />

    <ImageView
        android:id="@+id/imgPrivacy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="@+id/imgLetter"
        app:layout_constraintStart_toStartOf="@+id/imgLetter"
        app:layout_constraintTop_toBottomOf="@+id/imgLetter"
        app:srcCompat="@drawable/ic_new_privacy" />

    <ImageView
        android:id="@+id/imgChatRoom"
        android:layout_width="20dp"
        android:layout_height="22dp"
        android:visibility="gone"
        app:srcCompat="@drawable/ic_sign_out"
        tools:layout_editor_absoluteY="160dp" />

    <TextView
        android:id="@+id/btn_browser"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:drawablePadding="@dimen/_10sdp"
        android:fontFamily="@font/montserrat_alternates"
        android:gravity="center_vertical"
        android:padding="@dimen/_6sdp"
        android:text="@string/browser"
        android:textColor="@android:color/white"
        android:textSize="14sp"
        android:textStyle="bold"
        android:visibility="gone"
        tools:layout_editor_absoluteX="115dp"
        tools:layout_editor_absoluteY="160dp" />

    <TextView
        android:id="@+id/btn_home"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:drawablePadding="10dp"
        android:fontFamily="@font/montserrat_alternates"
        android:gravity="center_vertical"
        android:padding="6sp"
        android:text="@string/home"
        android:textColor="@android:color/white"
        android:textSize="14sp"
        android:textStyle="bold"
        android:visibility="gone"
        tools:layout_editor_absoluteX="230dp"
        tools:layout_editor_absoluteY="160dp" />

    <ImageView
        android:id="@+id/imgDashboard"
        android:layout_width="20dp"
        android:layout_height="22dp"
        android:visibility="gone"
        app:srcCompat="@drawable/ic_home2"
        tools:layout_editor_absoluteY="160dp" />

    <ImageView
        android:id="@+id/imgLogout2"
        android:layout_width="20dp"
        android:layout_height="22dp"
        app:layout_constraintBottom_toTopOf="@+id/imgLetter"
        app:layout_constraintEnd_toEndOf="@+id/imgLogout"
        app:layout_constraintStart_toStartOf="@+id/imgLogout"
        app:layout_constraintTop_toBottomOf="@+id/imgLogout"
        app:srcCompat="@drawable/ic_new_share" />

    <ImageView
        android:id="@+id/imgLogout"
        android:layout_width="20dp"
        android:layout_height="22dp"
        app:layout_constraintBottom_toTopOf="@+id/imgLogout2"
        app:layout_constraintEnd_toEndOf="@+id/imgShare"
        app:layout_constraintStart_toStartOf="@+id/imgShare"
        app:layout_constraintTop_toBottomOf="@+id/imgShare"
        app:srcCompat="@drawable/ic_rate_star" />

    <ImageView
        android:id="@+id/imgTicket"
        android:layout_width="20dp"
        android:layout_height="22dp"
        android:visibility="gone"
        app:srcCompat="@drawable/ic_call2"
        tools:layout_editor_absoluteY="160dp" />

    <TextView
        android:id="@+id/btn_removeads"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:drawablePadding="@dimen/_10sdp"
        android:fontFamily="@font/montserrat_alternates"
        android:gravity="center_vertical"
        android:maxLines="1"
        android:padding="@dimen/_6sdp"
        android:text="@string/removeads"
        android:textColor="@android:color/white"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imgShare2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imgShare2"
        app:layout_constraintTop_toTopOf="@+id/imgShare2" />

    <ImageView
        android:id="@+id/callBlock"
        android:layout_width="20dp"
        android:layout_height="22dp"
        app:layout_constraintBottom_toTopOf="@+id/imgShare"
        app:layout_constraintEnd_toEndOf="@+id/imgShare2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/imgShare2"
        app:layout_constraintTop_toBottomOf="@+id/imgShare2"
        app:srcCompat="@drawable/ic_call_blocker" />

    <ImageView
        android:id="@+id/imgShare"
        android:layout_width="20dp"
        android:layout_height="22dp"
        app:layout_constraintBottom_toTopOf="@+id/imgLogout"
        app:layout_constraintEnd_toEndOf="@+id/callBlock"
        app:layout_constraintStart_toStartOf="@+id/callBlock"
        app:layout_constraintTop_toBottomOf="@+id/callBlock"
        app:srcCompat="@drawable/ic_new_theme" />

    <TextView
        android:id="@+id/btn_theme"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:drawablePadding="@dimen/_10sdp"
        android:fontFamily="@font/montserrat_alternates"
        android:gravity="center_vertical"
        android:maxLines="1"
        android:padding="@dimen/_6sdp"
        android:text="@string/theme"
        android:textColor="@android:color/white"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imgShare"
        app:layout_constraintEnd_toEndOf="@+id/view23"
        app:layout_constraintStart_toEndOf="@+id/imgShare"
        app:layout_constraintTop_toTopOf="@+id/imgShare" />

    <TextView
        android:id="@+id/btn_privacy"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:drawablePadding="@dimen/_10sdp"
        android:fontFamily="@font/montserrat_alternates"
        android:gravity="center_vertical"
        android:maxLines="1"
        android:padding="@dimen/_6sdp"
        android:text="@string/menu_privacy"
        android:textColor="@android:color/white"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imgPrivacy"
        app:layout_constraintEnd_toEndOf="@+id/view23"
        app:layout_constraintStart_toEndOf="@+id/imgPrivacy"
        app:layout_constraintTop_toTopOf="@+id/imgPrivacy" />

    <TextView
        android:id="@+id/menuSettings"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:drawablePadding="@dimen/_10sdp"
        android:fontFamily="@font/montserrat_alternates"
        android:gravity="center_vertical"
        android:maxLines="1"
        android:padding="@dimen/_6sdp"
        android:text="@string/action_settings"
        android:textColor="@android:color/white"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="@+id/view23"
        app:layout_constraintStart_toEndOf="@+id/imgPrivacy"
        app:layout_constraintTop_toTopOf="@+id/imageView" />

    <TextView
        android:id="@+id/btn_contact_us"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:drawablePadding="@dimen/_10sdp"
        android:fontFamily="@font/montserrat_alternates"
        android:gravity="center_vertical"
        android:maxLines="1"
        android:padding="@dimen/_6sdp"
        android:text="@string/menu_contact"
        android:textColor="@android:color/white"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imgLetter"
        app:layout_constraintEnd_toEndOf="@+id/view23"
        app:layout_constraintStart_toEndOf="@+id/imgLetter"
        app:layout_constraintTop_toTopOf="@+id/imgLetter" />

    <ImageView
        android:id="@+id/imgNotification"
        android:layout_width="20dp"
        android:layout_height="22dp"
        android:visibility="gone"
        app:srcCompat="@drawable/ic_valut2"
        tools:layout_editor_absoluteY="160dp" />

    <ImageView
        android:id="@+id/imgProfile"
        android:layout_width="20dp"
        android:layout_height="22dp"
        android:visibility="gone"
        app:srcCompat="@drawable/ic_browser2"
        tools:layout_editor_absoluteY="160dp" />

    <TextView
        android:id="@+id/btn_call_blocker"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:drawablePadding="@dimen/_10sdp"
        android:fontFamily="@font/montserrat_alternates"
        android:gravity="center_vertical"
        android:padding="@dimen/_6sdp"
        android:text="@string/Call_blocker"
        android:textColor="@android:color/white"
        android:textSize="14sp"
        android:textStyle="bold"
        android:visibility="gone"
        tools:layout_editor_absoluteX="115dp"
        tools:layout_editor_absoluteY="160dp" />

    <TextView
        android:id="@+id/btn_vault"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:drawablePadding="@dimen/_10sdp"
        android:fontFamily="@font/montserrat_alternates"
        android:gravity="center_vertical"
        android:padding="@dimen/_6sdp"
        android:text="@string/Vault"
        android:textColor="@android:color/white"
        android:textSize="14sp"
        android:textStyle="bold"
        android:visibility="gone"
        tools:layout_editor_absoluteX="115dp"
        tools:layout_editor_absoluteY="160dp" />

    <ImageView
        android:id="@+id/imgShare2"
        android:layout_width="22dp"
        android:layout_height="22dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toStartOf="@+id/view23"
        app:layout_constraintTop_toTopOf="@+id/view23"
        android:src="@drawable/ic_new_diamond" />

    <TextView
        android:id="@+id/callBlockText"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:drawablePadding="@dimen/_10sdp"
        android:fontFamily="@font/montserrat_alternates"
        android:gravity="center_vertical"
        android:maxLines="1"
        android:padding="@dimen/_6sdp"
        android:text="@string/Call_blocker"
        android:textColor="@android:color/white"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/callBlock"
        app:layout_constraintEnd_toEndOf="@+id/view23"
        app:layout_constraintStart_toEndOf="@+id/callBlock"
        app:layout_constraintTop_toTopOf="@+id/callBlock" />


    <TextView
        android:id="@+id/btn_support"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:drawablePadding="@dimen/_10sdp"
        android:fontFamily="@font/montserrat_alternates"
        android:gravity="center_vertical"
        android:maxLines="1"
        android:padding="@dimen/_6sdp"
        android:text="@string/share"
        android:textColor="@android:color/white"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imgLogout2"
        app:layout_constraintEnd_toEndOf="@+id/view23"
        app:layout_constraintStart_toEndOf="@+id/imgLogout2"
        app:layout_constraintTop_toTopOf="@+id/imgLogout2" />

    <ImageView
        android:id="@+id/imgLetter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@+id/imgPrivacy"
        app:layout_constraintEnd_toEndOf="@+id/imgLogout2"
        app:layout_constraintStart_toStartOf="@+id/imgLogout2"
        app:layout_constraintTop_toBottomOf="@+id/imgLogout2"
        app:srcCompat="@drawable/ic_new_letter" />

    <TextView
        android:id="@+id/btn_rate"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:drawablePadding="@dimen/_10sdp"
        android:fontFamily="@font/montserrat_alternates"
        android:gravity="center_vertical"
        android:maxLines="1"
        android:padding="@dimen/_6sdp"
        android:text="@string/rate_us"
        android:textColor="@android:color/white"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imgLogout"
        app:layout_constraintEnd_toEndOf="@+id/view23"
        app:layout_constraintStart_toEndOf="@+id/imgLogout"
        app:layout_constraintTop_toTopOf="@+id/imgLogout" />

    <TextView
        android:id="@+id/chatRoom_menu"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:drawablePadding="@dimen/_10sdp"
        android:fontFamily="@font/montserrat_alternates"
        android:gravity="center_vertical"
        android:padding="@dimen/_6sdp"
        android:text="@string/menu_chat"
        android:textColor="@android:color/white"
        android:textSize="14sp"
        android:textStyle="bold"
        android:visibility="gone"
        tools:layout_editor_absoluteX="115dp"
        tools:layout_editor_absoluteY="160dp" />

    <ImageView
        android:id="@+id/btn_copy1"
        android:layout_width="0dp"
        android:layout_height="90dp"
        android:layout_marginTop="40dp"
        app:layout_constraintEnd_toEndOf="@+id/bgMosque"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/bgMosque"
        app:srcCompat="@drawable/ic_main_logo8" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/view23"
        app:layout_constraintEnd_toEndOf="@+id/imgPrivacy"
        app:layout_constraintStart_toStartOf="@+id/imgPrivacy"
        app:layout_constraintTop_toBottomOf="@+id/imgPrivacy"
        app:srcCompat="@drawable/ic_menu_settings" />

</androidx.constraintlayout.widget.ConstraintLayout>